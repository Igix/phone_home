    <div id="terms_unit" style="display: none;">
        {@
        type=>unit
        where=>1=1
        order=>d_add desc
        limit=>36
        offset=>0
        @}
    </div>
    <div id="terms_categories" style="display: none;">
        {@
        type=>categories
        where=>k_rubric_id = 1
        @}
    </div>
    <div class="container">
        <header>
            <div class="row">
		        <div class=" col-xs-6 col-sm-6 col-md-6 col-lg-6 ">
                    <a href="/" class="logo"><img  src="{{logo_self_src}}" /></a>
                </div>
		        <div class=" col-xs-6 col-sm-6 col-md-6 col-lg-6 ">
                    <div class=" col-xs-12 col-sm-12 col-md-12 col-lg-12 navbar-right">
		                <div class="row right_panel">
                    	    <div class="ava"></div>
                            <!--<div class="basket glyphicon glyphicon-shopping-cart"><div class="gb_Ma">{{shopping_basket_items_cnt}}</div></div>-->
                            <!--<div class="favorit glyphicon glyphicon-heart"> <div class="gb_Ma">2</div></div>-->
                            <div class="ask glyphicon glyphicon-question-sign" data-target='#feedback' data-toggle='modal' title="Задать вопрос"><div class="gb_Ma"></div></div>
                            <!--<div class="bell"><div class="gb_Ma">{{ticket_msg_cnt}}</div></div>-->
	                        <div class="name_user right">
                                {% if users_id_current == 0 %}
                                    <a href="#" data-target='#login' data-toggle='modal'>Войти <i class="glyphicon"></i></a>
                                {%else%}
                                    <span>{{user_name}}</span>
                                    <a href="#" data-target='#logout' data-toggle='modal'>Выход</a>
                                {% endif %}
                                <!--<a href="#">Игорь</a>-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row search_dread">
                <div class=" col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                     <div class="header_text center">
                        <!--<ol class="breadcrumb">
                          <li><a href="#">Thailand</a></li>
                          <li><a href="#">Chomburi</a></li>
                          <li><a href="#">Pattaya</a></li>
                          <li class="active">Hotels<span class="description"> (8678) вариантов размещения </span></li>
                        </ol>-->
                     </div>
                </div>
            </div>
        </header>

        <div class="row">
		    <div class=" col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                <h1 class="center"></h1>
                <div class="text_search_menu center">
                    <span>Поиск</span>&nbsp;
                    {% set tags_text_len = 11 %}
                    {% set tags_text = "ПоискЕще..." %}
                    {% for key,value in k_categories_arr %}
                        {% set tags_text = tags_text ~ value["name"] %}
                        {% if tags_text|length <= 75 %}
                            <a href="{{unit_list_url}}&kc_name={{value['name']}}" class="tags_list" data-k_categories_id="{{value['k_categories_id']}}">{{value["name"]}}</a>&nbsp;
                        {% endif %}
                    {% endfor %}
                    <!--<a href="#" class="tags_list_more" data-target='#tags_list' data-toggle='modal'>Еще...</a>-->
                </div>

                {% include 'search.twig' %}

                <!--box filters-->
                <div class="box_filters">
                    <span class="clear_all" style="display: none;"><a href="#">Очистить все</a></span>
                </div>
                <!--//box filters-->
            </div>
        </div>

        <!--box_catalog-->
        <div class="row">
    		<div id="sbiz5219" class="showbiz item">
    			<div style="height: 554px;" class="overflowholder">
    				<ul class="hlisting">
                        {% for key,value in unit_model_arr %}
                            <li class="item" data-unit_id="{{value['unit_id']}}" data-k_brand_id="{{value['k_brand_id']}}" data-k_essence_id="{{value['k_essence_id']}}"  data-k_categories_id="{{value['k_categories_id_arr']}}" data-k_properties_value_id="{{value['k_properties_value_id_arr']}}">
                     		    <div class="availability">
                     		    	<span class="in_stock">
                     		    		<span class="hint">В&nbsp;наличии</span>
                     		    	</span>
                     		    </div>
                     		    <div class="item-info">
                                    <div class="photo" style="text-align: center;">
                                          {% if value["img_card_primary"]|length > 0 %}
                                              <img alt="" src="{{value["img_card_primary"][0]}}/medium.png">
                                          {% else %}
                                              <img alt="" src="{{no_image_src}}">
                                          {% endif %}
                          			</div>
                     		    </div>
                     		    <div class="darkhover"></div>
                     		    <div class="detailholder">
                     		    	<div class="fn">{{value["name_detailed"]}}</div>
                     		    	<div class="divide20"></div>
                     		    	<p style="display: none; height: 66px; padding-top: 0px; margin-top: 0px; padding-bottom: 0px; margin-bottom: 0px;" class="description">
                                        {{value["properties_value_arr"]|raw}}
                     		    	</p>
                                    <div class="divide20"></div>
                     		    	<p></p>
                     		    	<div class="listing-action leftfloat">
                     		    		<span class="sell">Цена</span>
                     		    		<span class="price">{{value["cost"]}} руб</span>
                     		    	</div>
                     		    	<!--<div class="hreview-feedback rightfloat"><a href="#">READ MORE</a></div>-->
                     		    	<div class="sb-clear"></div>
                     		    </div>
                     		</li>
                        {% endfor %}
    				</ul>

                    <div class="sbclear"></div>
                </div>
                <div class="sbclear"></div>
            </div>
    	</div>
        <!-- // box_catalog-->
    </div>

        <div class="row">
		    <div class="col-sm-12 col-md-12 col-lg-12 ">
                <input type="button" class=" btn btn-default center more" id="btn_unit_more" value="Показывать еще 36">
            </div>
        </div>

        <!--subskribe-->
        <div class="row subs">
		    <div class="col-sm-0 col-md-1 col-lg-1 "> </div>
		    <div class=" col-xs-5 col-sm-4 col-md-4 col-lg-4 unsubs ">
